{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "paths": {
      "ngx-formfy": [
        "./dist/ngx-formfy"
      ],
      "@spartan-ng/helm/form-field": [
        "./dist/ui-form-field-helm"
