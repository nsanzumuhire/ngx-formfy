      ],
      "@spartan-ng/helm/radio-group": [
        "./dist/ui-radio-group-helm"
      ],
      "@spartan-ng/helm/date-picker": [
        "./dist/ui-date-picker-helm"
      ],
    },
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve"
